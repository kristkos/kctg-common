{"name":"Amber","type":"treasure","img":"modules/kctg-common/images/Amber.png","system":{"description":{"value":"<p>Fossilized pine resin, can be polished and used for ornamental purposes such as jewelry beads. There a is high demand for amber by alchemists as it can trap small forgotten creatures.</p>"},"source":{"value":""},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.788,"lastMigration":{"datetime":"2022-09-14T19:03:14.685+03:00","version":{"schema":null,"foundry":"10.285","system":"4.0.4"}}},"quantity":1,"baseItem":null,"hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"-"},"equippedBulk":{"value":""},"price":{"value":{"gp":5}},"equipped":{"carryType":"worn"},"stackGroup":"gems","negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":null},"preciousMaterialGrade":{"value":null},"size":"med","identification":{"status":"identified","unidentified":{"name":"Unusual Treasure","img":"systems/pf2e/icons/unidentified_item_icons/adventuring_gear.webp","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":""},"level":{"value":0}},"effects":[],"flags":{"core":{"sourceId":"Item.YMIFMpmI3TWhIUx6"}},"_stats":{"systemId":"pf2e","systemVersion":"4.0.4","coreVersion":"10.285","createdTime":1663171394688,"modifiedTime":1663274765102,"lastModifiedBy":"C2FNPJk1lw4E69Pu"},"folder":null,"sort":0,"ownership":{"default":0,"C2FNPJk1lw4E69Pu":3},"_id":"9Jj6Z376JJywKV2c"}
